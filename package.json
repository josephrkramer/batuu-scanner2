{
  "name": "batuu-scanner2",
  "version": "1.0.0",
  "description": "Javascript Batuu Code Scanner based on [Html5-QRCode](https://github.com/mebjas/html5-qrcode). With additions of Galaxy's Edge Aztec Code mapping based on [Andrew Mohawk's Star Wars Aztec Barcodes](https://github.com/AndrewMohawk/StarwarsAztec). Puzzle based on [15 Puzzle by arnisritins](https://github.com/arnisritins/15-Puzzle).",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "lint": "prettier . --check && eslint . --ext .ts",
    "lint-fix": "prettier . --write && eslint . --ext .ts --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephrkramer/batuu-scanner2.git"
  },
  "author": "Joseph Kramer",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/josephrkramer/batuu-scanner2/issues"
  },
  "homepage": "https://github.com/josephrkramer/batuu-scanner2#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^24.1.0",
    "prettier": "3.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.12",
    "vitest": "^1.5.2"
  },
  "dependencies": {
    "dayjs": "^1.11.11",
    "html5-qrcode": "^2.3.8"
  }
}
